<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.inv.invmiscout.mapper.InvMiscOutDetailMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  inv_misc_out_detail 
		WHERE
			 pid = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.inv.invmiscout.entity.InvMiscOutDetail">
		SELECT * 
		FROM  inv_misc_out_detail
		WHERE
			 pid = #{mainId} 	</select>
</mapper>
